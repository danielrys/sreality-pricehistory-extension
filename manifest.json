{
  "name": "Realty price checker",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "menu.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["pageContent.js"],
      "matches": ["https://www.sreality.cz/detail/prodej/*"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab", "storage"]
}
